{
  "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },

  "env": {
    "browser": true,
    "es6": true
  },

  "rules": {
    "for-direction": 2,
    "getter-return": 2,
    "no-trailing-spaces": 2,
    "no-multi-spaces": 2,
    "no-constant-condition": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-extra-semi": 2,
    "semi": 2,
    "eol-last": 2,
    "no-unreachable": 2,
    "linebreak-style": [ 2, "unix" ],
    "max-len": [ 2, { "code": 80 } ],
    "indent": [2, 2]
  }  
}
